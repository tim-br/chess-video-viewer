                </li>

                <li id="home-text">
                    <div> Home Page</div>
                </li>

            </ul>
        </div>
        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->

        <div id="page-content-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="center-block">
                        <div id="hide-div">
                        <!-- <h1 class="launch-div">Top Level Chess</h1> -->
