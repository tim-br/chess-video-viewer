                            <h3 class='active-semester' id='search'> Search/Modify Videos/ Lessons</h3>
                            <h4>Week Number</h4>
                            <select name="cars">

                            <% (1..15).each do |t| %>
                              <option value="<%= t %>"><%= t%></option>
                            <% end %>
                            </select>

                            <div id="form">

                            </div>

                        </div>

                        </div>
