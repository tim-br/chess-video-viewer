<body>

    <div id="wrapper">

        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                    <a href="#">
                        Top Level Chess
                    </a>
                </li>

                <li id="beginner" class="level">
                    <div>Beginner</div>
                </li>

                <li id="advanced" class="level">
